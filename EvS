package com.nembulov.android.wp

import android.content.Intent
import androidx.appcompat.app.AppCompatActivity
import android.os.Bundle
import android.widget.Button
import android.widget.ImageButton
import android.widget.LinearLayout

class MainActivity : AppCompatActivity() {
    override fun onCreate(savedInstanceState: Bundle?) {
        super.onCreate(savedInstanceState)

        setContentView(R.layout.activity_main)
        var ib_html = findViewById<ImageButton>(R.id.ib_html)
        ib_html.setOnClickListener {
            startActivity(Intent(this@MainActivity, HTML::class.java))
        }
        var ib_css = findViewById<ImageButton>(R.id.ib_css)
        ib_css.setOnClickListener {
            startActivity(Intent(this@MainActivity, CSS::class.java))
        }
//        var ib_js = findViewById<ImageButton>(R.id.ib_js)
//        ib_js.setOnClickListener {
//            startActivity(Intent(this@MainActivity, JS::class.java))
//        }
    }
}
